{
  "name": "shopping-cart-application",
  "version": "1.0.0",
  "description": "A imple Shopping Cart Application",
  "scripts": {
    "start": "node dist/src/server.js",
    "prestart": "npm run build-ts",
    "build-ts": "npm run clean && tsc",
    "clean": "rimraf dist/",
    "watch-ts": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint src/**/*.ts --fix",
    "test": "NODE_ENV=test jest --forceExit --coverage --verbose --colors --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed",
    "seed:all": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all"
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "transform": {
      "^.+\\.[jt]sx?$": "ts-jest"
    },
    "setupFiles": [
      "dotenv/config"
    ],
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  },
  "author": "Ibiniyi Obikoya",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "emailjs": "^3.4.0",
    "events": "^3.2.0",
    "express": "^4.17.1",
    "faker": "^5.3.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.20",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.4.0",
    "supertest": "^6.1.3",
    "typescript": "^4.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.10",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.4.4",
    "tslint": "^5.20.1"
  }
}
